{
  "name": "ecs-backend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "setup-db": "node setup-arangodb.js",
    "dump-data": "node scripts/dump-data.js",
    "import-data": "node scripts/import-data.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecs-backend",
    "pm2:restart": "pm2 restart ecs-backend",
    "pm2:delete": "pm2 delete ecs-backend",
    "pm2:status": "pm2 status",
    "pm2:logs": "pm2 logs ecs-backend",
    "deploy": "npm run setup-db && npm run import-data && npm run pm2:start",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "dependencies": {
    "arangojs": "^10.1.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5"
  }
}
